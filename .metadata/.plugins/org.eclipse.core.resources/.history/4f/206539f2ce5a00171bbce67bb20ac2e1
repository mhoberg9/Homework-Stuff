package de.tu.prog02.klassenhierachien

class Duck(override val name:String) extends Bird(name) with Flying with Swimming{
  override def makeNoise()=super.makeNoise()+s" superDuck: $name";
  def swimm()="Performing swimming";
def fly()= "Performing flying";
   val swimmMessage="Quaking while f";
  val flyMessage="Quak";
}